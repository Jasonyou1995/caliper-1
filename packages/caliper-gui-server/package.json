{
    "name": "@hyperledger/caliper-gui-server",
    "description": "Hyperledger Caliper GUI server module, for easily assembling configuration files and monitoring benchmark runs",
    "version": "0.3.0-unstable",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/hyperledger/caliper",
        "directory": "packages/caliper-gui"
    },
    "main": "index.js",
    "scripts": {
        "pretest": "npm run licchk",
        "licchk": "license-check-and-add",
        "test": "npm run lint",
        "lint": "npx eslint .",
        "nyc": "nyc mocha --recursive -t 10000",
        "start": "nodemon ./app.js --exec babel-node -e js",
        "debug": "DEBUG=express:* nodemon ./index.js --exec babel-node -e js"
    },
    "engines": {
        "node": ">=8.10.0",
        "npm": ">=5.6.0"
    },
    "dependencies": {
        "@hyperledger/caliper-burrow": "0.2.0",
        "@hyperledger/caliper-composer": "0.2.0",
        "@hyperledger/caliper-core": "0.2.0",
        "@hyperledger/caliper-fabric": "0.2.0",
        "@hyperledger/caliper-iroha": "0.2.0",
        "@hyperledger/caliper-sawtooth": "0.2.0",
        "body-parser": "^1.19.0",
        "chalk": "3.0.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "js-yaml": "^3.13.1",
        "mime-types": "^2.1.26",
        "mongodb": "^3.5.4",
        "mongoose": "^5.9.2",
        "multer": "^1.4.2",
        "nodemon": "^2.0.2",
        "shelljs": "^0.8.3",
        "yargs": "15.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.0.0",
        "@babel/plugin-proposal-do-expressions": "^7.0.0",
        "@babel/plugin-proposal-export-default-from": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-bind": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.0.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-optional-chaining": "^7.0.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "babel-eslint": "10.1.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "license-check-and-add": "3.0.4",
        "mocha": "7.1.0",
        "nyc": "15.0.0",
        "rewire": "^4.0.1",
        "sinon": "^9.0.0"
    },
    "license-check-and-add-config": {
        "folder": ".",
        "license": "../../LICENSE.txt",
        "exact_paths_method": "EXCLUDE",
        "exact_paths": [
            "node_modules",
            ".nyc_output",
            "coverage",
            ".gitignore",
            "log",
            ".jshintrc"
        ],
        "file_type_method": "EXCLUDE",
        "file_types": [
            ".yml",
            ".md",
            ".html",
            ".php",
            ".log",
            ".css",
            ".scss",
            ".map",
            ".eot",
            ".ttf",
            ".woff",
            ".woff2"
        ],
        "insert_license": false,
        "license_formats": {
            "js|jsx": {
                "prepend": "/*",
                "append": "*/",
                "eachLine": {
                    "prepend": "* "
                }
            },
            "editorconfig|yaml": {
                "prepend": "#",
                "append": "#",
                "eachLine": {
                    "prepend": "# "
                }
            }
        }
    },
    "nyc": {
        "exclude": [
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": false,
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
    },
    "license": "Apache-2.0"
}
