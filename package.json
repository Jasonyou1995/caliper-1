		"license-check-and-add": "2.3.6"
	},
	"license-check-and-add-config": {
		"folder": ".",
		"license": "LICENSE.txt",
		"exact_paths_method": "EXCLUDE",
		"exact_paths": [
			"CODEOWNERS",
			"LICENSE",
			".idea",
			"node_modules",
			".nyc_output",
			"coverage",
			"scripts",
			".git",
			".gitignore",
			"packages",
			"log"
		],
		"file_type_method": "EXCLUDE",
		"file_types": [
			".yml",
			".md",
			".log",
			".html"
		],
		"insert_license": false,
		"license_formats": {
			"js|cto|qry|go": {
				"prepend": "/*",
				"append": "*/",
				"eachLine": {
					"prepend": "* "
				}
			},
			"cfg|editorconfig|yaml|py": {
				"prepend": "#",
				"append": "#",
				"eachLine": {
					"prepend": "# "
				}
			}
		}
	},
	"license": "Apache-2.0"
